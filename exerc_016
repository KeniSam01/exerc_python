#operadores aritméticos

soma = 45 + 12
subtracao = 45 - 12
div = 45 / 12
multi = 45 * 12
potencia = 45 ** 12
div_int = 45 // 12
rest_div = 45 % 12

print(soma)
print(subtracao)
print(div)
print(multi)
print(potencia)
print(div_int)
print(rest_div)

# ordem de precedência

# 1 = ()
# 2 = **
# 3 = * / // %
# 4 = + -

print(5+3*2)
print(3*5+4**2)
print(3*(5+4)**2)